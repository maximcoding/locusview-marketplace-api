  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx tsc -p tsconfig.build.json\n",
    "heroku-prebuild": "rm -rf dist node_modules redis-data data",
    "heroku-postbuild": "npm install && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=production && node dist/main",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:dev:windows": "set NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=testing jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "npm run build && node dist/seeder",
    "seed:refresh": "node dist/seeder --refresh"
  },
